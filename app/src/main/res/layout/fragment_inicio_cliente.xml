<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/degradado2"
    tools:context=".Activities.InicioCliente">

    <ImageView
        android:id="@+id/iconSalir_fragmentInicio"
        android:layout_width="37dp"
        android:layout_height="34dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="25dp"
        android:layout_x="15dp"
        android:layout_y="21dp"
        android:background="@drawable/back" />

    <ImageView
        android:id="@+id/notificacion_fragmentInicio"
        android:layout_width="37dp"
        android:layout_height="30dp"
        android:layout_marginLeft="360dp"
        android:layout_marginTop="25dp"
        android:layout_x="349dp"
        android:layout_y="29dp"
        android:src="@mipmap/notificacion_icon" />

    <TextView
        android:id="@+id/txtView_InicioTienda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_x="-3dp"
        android:layout_y="23dp"
        android:paddingLeft="160dp"
        android:text="Inicio"
        android:textColor="@color/logo_background"
        android:textSize="30dp"></TextView>


</AbsoluteLayout>